FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "3";
    object      cellLevel;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    Wand_Inlet
    {
        type            calculated;
        value           uniform 0;
    }
}


