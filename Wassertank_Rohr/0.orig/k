FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.0008;

boundaryField
{
   
   "wall.*"
    {
        type            kqRWallFunction;
        value           uniform 0.0008;
    }
  
    inlet
    {
        type            fixedValue;
        value           uniform 0.0008;
    }
    outlet
    {
        type            inletOutlet;
	inletValue uniform 0.0008;
	value uniform 0.0008;
    }
   

}


